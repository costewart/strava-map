,"/Users/charlottestewart/strava-map/src/reportWebVitals.js",[],"/Users/charlottestewart/strava-map/src/App.js",["94","95","96","97","98","99","100"],"/Users/charlottestewart/strava-map/src/components/TopBar/index.js",[],"/Users/charlottestewart/strava-map/src/components/TopBar/TopBar.js",[],"/Users/charlottestewart/strava-map/src/components/Map/index.js",[],"/Users/charlottestewart/strava-map/src/components/Map/Map.js",["101","102","103","104"],"import React, { useEffect, useState } from \"react\";\nimport FilterBox from \"../FilterBox\";\nimport { MapContainer, TileLayer, Polyline, Popup } from \"react-leaflet\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { selectors, actions } from \"../../redux/activities\";\nimport \"./Map.css\";\n\nconst Map = () => {\n  const [activities, setActivities] = useState(\n    useSelector(selectors.getActivities)\n  );\n\n  const getSports = () => {\n    let sports = {};\n    activities.forEach((activity) => {\n      if (!sports[activity.type]) {\n        sports[activity.type] = true;\n      }\n    });\n    return sports;\n  };\n\n  const [filters, setFilters] = useState(getSports());\n\n  const removeFalsy = (obj) => {\n    let newObj = {};\n    Object.keys(obj).forEach((prop) => {\n      if (obj[prop]) {\n        newObj[prop] = obj[prop];\n      }\n    });\n    return newObj;\n  };\n\n  const sports = Object.keys(removeFalsy(filters));\n\n  return (\n    <div>\n      <FilterBox onSubmit={setFilters} sports={filters} />\n      <MapContainer\n        center={[49.246292, -123.116226]}\n        zoom={11}\n        scrollWheelZoom={true}\n      >\n        <TileLayer\n          attribution='&copy; <a href=\"http://osm.org/copyright\">OpenStreetMap</a> contributors'\n          url=\"https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png\"\n        />\n\n        {activities\n          .filter((activity) => sports.includes(activity.type))\n          .map((activity, index) => (\n            <Polyline\n              key={index}\n              positions={activity.positions}\n              pathOptions={{ color: activity.color }}\n            >\n              <Popup positions={activity.positions}>\n                {activity.name}\n                {activity.color}\n              </Popup>\n            </Polyline>\n          ))}\n      </MapContainer>\n    </div>\n  );\n};\nexport default Map;\n","/Users/charlottestewart/strava-map/src/components/Home/index.js",[],"/Users/charlottestewart/strava-map/src/components/Home/Home.js",["105"],"import React from \"react\";\nimport Footer from \"../Footer\";\nimport \"./Home.css\";\nimport connect from \"./connectwithstrava.png\";\n\nconst client_id = 65712;\nconst connectWithStrava = async () => {\n  console.log(\"Connecting to Strava...\");\n\n  // client_id for my Strava application as listed at https://www.strava.com/settings/api when logged in\n  window.location.replace(\n    `https://www.strava.com/oauth/authorize?client_id=${client_id}&response_type=code&redirect_uri=http://localhost:5000/authenticate&approval_prompt=force&scope=activity:read`\n  );\n};\n\nconst Home = () => {\n  return (\n    <div className=\"Home\">\n      <div className=\"title\">\n        <h1>Map Maker</h1>\n      </div>\n      <div className=\"description\">\n        Create your own comprehensive map of your strava routes - choose to\n        display any or all your sports, from day 1 or some time in between. A\n        great map to visualize your activity in an area, and to share with\n        friends.\n      </div>\n\n      <img src={connect} alt=\"my image\" onClick={connectWithStrava} />\n\n      <Footer />\n    </div>\n  );\n};\nexport default Home;\n","/Users/charlottestewart/strava-map/src/components/Create/index.js",[],"/Users/charlottestewart/strava-map/src/components/Create/Create.js",["106","107","108"],"import React, { useState } from \"react\";\nimport { useHistory } from \"react-router-dom\";\nimport TopBar from \"../TopBar\";\nimport Cookies from \"js-cookie\"; // do we need this?\nimport polyline from \"@mapbox/polyline\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { selectors, actions } from \"../../redux/activities\";\nconst colors = {\n  Ride: \"#6495ED\",\n  Run: \"#E9967A\",\n  Swim: \"#1E90FF\",\n  Walk: \"#DAA520\",\n  Hike: \"#228B22\",\n  AlpineSki: \"#DCDCDC\",\n  BackcountrySki: \"#DCDCDC\",\n  Canoe: \"#B22222\",\n  Crossfit: \"#2F4F4F\",\n  Kitesurf: \"#00FFFF\",\n  Surfing: \"#556B2F\",\n};\n\nconst Create = () => {\n  const history = useHistory();\n\n  const dispatch = useDispatch();\n\n  const getActivities = async () => {\n    var access_token = Cookies.get(\"str-zoom-access_token\");\n    var refresh_token = Cookies.get(\"str-zoom-refresh_token\");\n\n    const settings = {\n      method: \"GET\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n        Authorization: access_token,\n        \"refresh-token\": refresh_token,\n      },\n    };\n\n    var response = await fetch(\"http://localhost:5000/activities\", settings);\n    const data = await response.json();\n\n    const polylines = [];\n    for (let i = 0; i < data.length; i += 1) {\n      const activity_polyline = data[i].map.summary_polyline;\n      const activity_name = data[i].name;\n      const activity_type = data[i].type;\n\n      polylines.push({\n        positions: polyline.decode(activity_polyline),\n        name: activity_name,\n        type: activity_type,\n        color: colors[activity_type],\n      });\n    }\n\n    console.log(polylines);\n    dispatch(actions.setActivities(polylines));\n\n    history.push(\"/map\");\n\n    return data;\n  };\n\n  return (\n    <div>\n      <TopBar /> <button onClick={getActivities}>Get Activities </button>\n    </div>\n  );\n};\nexport default Create;\n","/Users/charlottestewart/strava-map/src/components/Footer/index.js",[],"/Users/charlottestewart/strava-map/src/components/Footer/Footer.js",[],"/Users/charlottestewart/strava-map/src/redux/index.js",[],"/Users/charlottestewart/strava-map/src/redux/activities.js",["109"],"export const types = {\n  SET_ACTIVITIES: \"ui/set_activities\",\n  SET_SPORTS: \"ui/set_sports\",\n};\n\nconst initialState = {\n  activities: [],\n  sports: {\n    Ride: true,\n    Kitesurf: false,\n    Surfing: true,\n  },\n};\n\nexport default (state = initialState, action) => {\n  const { type, payload } = action;\n\n  switch (type) {\n    case types.SET_ACTIVITIES: {\n      const { activities } = payload;\n      return {\n        ...state,\n        activities,\n      };\n    }\n\n    case types.SET_SPORTS: {\n      const { sports } = payload;\n      return {\n        ...state,\n        sports,\n      };\n    }\n\n    default:\n      return state;\n  }\n};\n\nconst getActivities = (state) => state.activities.activities;\nconst getSports = (state) => state.activities.sports;\n\nexport const selectors = {\n  getActivities,\n  getSports,\n};\n\nconst setActivities = (activities) => ({\n  type: types.SET_ACTIVITIES,\n  payload: { activities },\n});\n\nconst setSports = (sports) => ({\n  type: types.SET_SPORTS,\n  payload: { sports },\n});\n\nconst actions = {\n  setActivities,\n  setSports,\n};\n\nexport { actions };\n","/Users/charlottestewart/strava-map/src/components/FilterBox/index.js",[],"/Users/charlottestewart/strava-map/src/components/FilterBox/FilterBox.js",[],"/Users/charlottestewart/strava-map/src/components/SportCheckbox/index.js",[],"/Users/charlottestewart/strava-map/src/components/SportCheckbox/SportCheckbox.js",[],"/Users/charlottestewart/strava-map/src/components/Intro/index.js",[],"/Users/charlottestewart/strava-map/src/components/Intro/Intro.js",["110","111","112"],"import React from \"react\";\nimport \"./Intro.css\";\nimport sample from \"./sample.jpeg\";\n\nconst Intro = () => {\n  return (\n    <div className=\"intro\">\n      <div class=\"grid-container\">\n        <div class=\"grid-item\">\n          <div className=\"headline\">Explore, Track and Map</div>\n          <div className=\"large-text\">\n            Your all in one solution to mapping your adventures and sharing with\n            friends.\n          </div>\n          <button>Create Your Map</button>\n        </div>\n        <div class=\"grid-item-image\">\n          <img src={sample} alt=\"my image\" />\n        </div>\n        <div class=\"grid-item-image\">\n          <img src={sample} alt=\"my image\" />\n        </div>\n        <div class=\"grid-item\">\n          <div className=\"small-header\">\n            A new seamless way to track your lifestyle all in one place\n          </div>\n          <div className=\"paragraph\">\n            Tracking your active lifestyle has never been easier. Let Map Maker\n            combine all desired activities and routes into one visual map.\n            Executed to perfection and personalization. Time to get tracking!\n          </div>\n        </div>\n        <div class=\"grid-item\">\n          <div className=\"small-header\">\n            Easily share maps and stay connected with others\n          </div>\n          <div className=\"paragraph\">\n            Share your map activity with friends. Gain ideas, tips and advice\n            from your fellow Map Makers by exploring together!\n          </div>\n        </div>\n        <div class=\"grid-item-image\">\n          <img src={sample} alt=\"my image\" />\n        </div>\n        <div class=\"grid-item\">\n          <div className=\"small-header\">\n            Combine all your Strava routes to display on one map\n          </div>\n          <div className=\"paragraph\">\n            Create your own comprehensive map of your strava routes. Choose to\n            display any or all your sports. Map Maker is a great map to\n            visualize your activity in an area, and to share with friends.\n          </div>\n        </div>\n        <div class=\"grid-item-image\">\n          <button>Create Your Map</button>\n        </div>\n      </div>\n    </div>\n  );\n};\nexport default Intro;\n",{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","severity":1,"message":"118","line":6,"column":50,"nodeType":"119","messageId":"120","endLine":6,"endColumn":54},{"ruleId":"117","severity":1,"message":"121","line":7,"column":40,"nodeType":"119","messageId":"120","endLine":7,"endColumn":55},{"ruleId":"117","severity":1,"message":"122","line":31,"column":9,"nodeType":"119","messageId":"120","endLine":31,"endColumn":22},{"ruleId":"117","severity":1,"message":"123","line":32,"column":9,"nodeType":"119","messageId":"120","endLine":32,"endColumn":24},{"ruleId":"117","severity":1,"message":"124","line":33,"column":9,"nodeType":"119","messageId":"120","endLine":33,"endColumn":18},{"ruleId":"117","severity":1,"message":"125","line":34,"column":9,"nodeType":"119","messageId":"120","endLine":34,"endColumn":22},{"ruleId":"117","severity":1,"message":"126","line":35,"column":9,"nodeType":"119","messageId":"120","endLine":35,"endColumn":14},{"ruleId":"117","severity":1,"message":"127","line":1,"column":17,"nodeType":"119","messageId":"120","endLine":1,"endColumn":26},{"ruleId":"117","severity":1,"message":"128","line":4,"column":23,"nodeType":"119","messageId":"120","endLine":4,"endColumn":34},{"ruleId":"117","severity":1,"message":"129","line":5,"column":21,"nodeType":"119","messageId":"120","endLine":5,"endColumn":28},{"ruleId":"117","severity":1,"message":"130","line":9,"column":22,"nodeType":"119","messageId":"120","endLine":9,"endColumn":35},{"ruleId":"131","severity":1,"message":"132","line":29,"column":7,"nodeType":"133","endLine":29,"endColumn":71},{"ruleId":"117","severity":1,"message":"134","line":1,"column":17,"nodeType":"119","messageId":"120","endLine":1,"endColumn":25},{"ruleId":"117","severity":1,"message":"135","line":6,"column":10,"nodeType":"119","messageId":"120","endLine":6,"endColumn":21},{"ruleId":"117","severity":1,"message":"136","line":7,"column":10,"nodeType":"119","messageId":"120","endLine":7,"e